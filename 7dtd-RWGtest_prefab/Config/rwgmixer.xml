<!-- Lower the probability of this hub being used -->
<!-- <set xpath="/rwgmixer/cell_rules/cell_rule[@name='default']/hub_rule[@name='default']/@prob">0.1</set>" -->

<!-- flatten things a little so there are more towns -->

<set xpath="/rwgmixer/terrain_generators/terrain_generator[@name='vanilla']/module[@name='wrld_Refiner0']/property[@name='scale']/@value">120</set>
<set xpath="/rwgmixer/terrain_generators/terrain_generator[@name='vanilla']/module[@name='wrld_Refiner1']/property[@name='scale']/@value">100</set>
<set xpath="/rwgmixer/terrain_generators/terrain_generator[@name='vanilla']/module[@name='wtrTerra_Sb2']/property[@name='scale']/@value">-3</set> 
<set xpath="/rwgmixer/terrain_generators/terrain_generator[@name='vanilla']/module[@name='MasterScaleBias']/property[@name='bias']/@value">20</set>

<!-- small increase to water level -->
<set xpath="/rwgmixer/terrain_generators/terrain_generator[@name='vanilla']/@water_level">40</set>

<append xpath="/rwgmixer/prefab_rules/prefab_rule[@name='townGroup']">
	<prefab rule="COMPOPACK"/>
</append>


<!-- SET ALL HUBS TO BE USED IN RWG -->
<append xpath="/rwgmixer/cell_rules">
		<cell_rule name="default">
      <hub_rule name="default" prob="1" />
			<wilderness_rule name="default" prob="1"/>
		</cell_rule>
    
		<cell_rule name="compopack_cities_big">
			<hub_rule name="compopacktown_big" prob="0.7" />
			<hub_rule name="asiatown_big" prob="0.2" />
			<hub_rule name="default" prob="0.1" />			
			<wilderness_rule name="default" prob="1"/>
		</cell_rule>
		
		<cell_rule name="compopack_cities_small">
			<hub_rule name="asiatown_small" prob="0.6" />
			<hub_rule name="compopacktown_small" prob="0.25" />
			<hub_rule name="default" prob="0.15" />
			<wilderness_rule name="default" prob="1"/>
		</cell_rule>
</append>

<!-- hub system legend:
    F = move forward in facing direction
    + = turn right 90 degrees
    - = turn left 90 degrees
    + + = 180 degrees
    - - = 180 degress
    [ = Save position and facing direction
    ] = return to last saved position and facing direction (if preceded by a [ )
    * = pick from the alt_commands list
-->

<!-- HUB DEFINITIONS -->
<append xpath="/rwgmixer/hub_rules">  
  <!-- ASIA TOWN -->
  <hub_rule name="asiatown_big">
		<downtown_zone_size_perc value="0.7"/>
    <prefab_rule name="asiatownPOIS"/>
    <street_gen level="25" length_multiplier="7">
      <axiom value="[FFCT][-FFT][+FFI][--FFN][+FFT][-FFI][++FN][F-FF+FF[-T]FF+FF+FFFF-FF-FF[+I]FF-F[+N]"/>
      <rule char="T" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
      <rule char="I" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FFFF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
      <rule char="N" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
      <rule char="K" replace_with="[FFCT][-FFT][+FFI][--FFN][+FFT][-FFI][++FN]"/>
      <alt_commands chars="T,I,N,K"/> 
    </street_gen>
  </hub_rule>		
	<!-- COMPOPACK TOWN -->
	<hub_rule name="compopacktown_big">
    <downtown_zone_size_perc value="0.8"/>
    <prefab_rule name="compopacktownPOIS"/>
    <street_gen level="25" length_multiplier="8">
      <axiom value="[FFCT][-FFT][+FFI][--FFN][+FFT][-FFI][++FN][F-FF+FF[-T]FF+FF+FFFF-FF-FF[+I]FF-F[+N]"/>
      <rule char="T" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
      <rule char="I" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
      <rule char="N" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
      <rule char="K" replace_with="[FFCT][-FFT][+FFI][--FFN][+FFT][-FFI][++FN]"/>
      <alt_commands chars="T,I,N,K"/> 
    </street_gen>
  </hub_rule>	
	
	<!-- ASIA TOWN -->
	<hub_rule name="asiatown_small">
		<downtown_zone_size_perc value="0.5"/>
		<prefab_rule name="asiatownPOIS"/>
		<street_gen level="25" length_multiplier="5">
			<axiom value="[FFCT][-FFT][+FFI][--FFN][+FFT][-FFI][++FN][F-FF+FF[-T]FF+FF+FFFF-FF-FF[+I]FF-F[+N]"/>
			<rule char="T" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
			<rule char="I" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
			<rule char="N" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
			<rule char="K" replace_with="[FFCT][-FFT][+FFI][--FFN][+FFT][-FFI][++FN]"/>
			<alt_commands chars="T,I,N,K"/> 
		</street_gen>
	</hub_rule>		
	<!-- COMPOPACK TOWN -->
	<hub_rule name="compopacktown_small">
		<downtown_zone_size_perc value="0.6"/>
		<prefab_rule name="compopacktownPOIS"/>
		<street_gen level="25" length_multiplier="6">
			<axiom value="[FFCT][-FFT][+FFI][--FFN][+FFT][-FFI][++FN][F-FF+FF[-T]FF+FF+FFFF-FF-FF[+I]FF-F[+N]"/>
			<rule char="T" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FFFF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
			<rule char="I" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
			<rule char="N" replace_with="F-FF+FF[-T**]FF+FF+FFFF-FF-FF[+I**]FF-F[+N*K]"/>
			<rule char="K" replace_with="[FFCT][-FFT][+FFI][--FFN][+FFT][-FFI][++FN]"/>
			<alt_commands chars="T,I,N,K"/> 
		</street_gen>
	</hub_rule>	


</append>


