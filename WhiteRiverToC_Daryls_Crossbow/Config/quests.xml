<config>
	<append xpath="/quests" >
			<quest id="+Veteran Quest For Daryl's Zombiekiller Crossbow">
				<property name="name_key" value="vetquest_toc_daryl_name"/>
				<property name="subtitle_key" value="vetquest_toc_daryl_subtitle"/>
				<property name="description_key" value="vetquest_toc_daryl_description"/>
				<property name="icon" value="ui_game_symbol_map_trader" />
				<property name="repeatable" value="true" />
				<property name="category_key" value="quest" />
				<property name="difficulty" value="easy" />
				<property name="offer_key" value="vetquest_toc_daryl_offer"/>
				<property name="statement_key" value="vetquest_toc_daryl_statement"/>
				<property name="response_key" value="vetquestdialog_toc_daryl"/>				
				<property name="group_name_key" value="vetquest_toc_daryl_group"/>
				<property name="shareable" value="false" />				
	
				<objective type="FetchKeep" id="gunBowT1WoodenBowParts" value="2" phase="1" />
				<objective type="FetchKeep" id="gunBowT0PrimitiveBow" value="20" phase="1" />
				<objective type="FetchKeep" id="resourceWood" value="400" phase="1" />
				<objective type="FetchKeep" id="resourcePaper" value="1" phase="1" />
				<objective type="Craft" id="vetwrititem_toc_daryl_name" value="1" phase="2" />		
	
				<reward type="Exp" value="1000" />
			</quest>
			<quest id="+Veteran Reward of Daryl's Zombiekiller Crossbow">
				<property name="name_key" value="vetquestreward_toc_daryl_name"/>
				<property name="subtitle_key" value="vetquestreward_toc_daryl_subtitle"/>
				<property name="description_key" value="vetquestreward_toc_daryl_description"/>
				<property name="icon" value="ui_game_symbol_map_trader" />
				<property name="repeatable" value="true" />
				<property name="category_key" value="quest" />
				<property name="difficulty" value="easy" />
				<property name="offer_key" value="vetquestreward_toc_daryl_offer"/>
				<property name="statement_key" value="vetquestreward_toc_daryl_statement"/>
				<property name="response_key" value="vetquestrewarddialog_toc_daryl"/>				
				<property name="group_name_key" value="vetquest_toc_daryl_group"/>
				<property name="shareable" value="false" />						
				<property name="completiontype" value="TurnIn" />
	
				<objective type="Goto" value="5" phase="1" />
				<objective type="ReturnToNPC">
					<property name="phase" value="2"/>
					<property name="nav_object" value="return_to_trader" />
				</objective>
				<objective type="InteractWithNPC">
					<property name="phase" value="3" />
					<property name="nav_object" value="return_to_trader" />										
				</objective>
				
				<reward type="Item" id="questitem_toc_citizencard_name" value="1" />	<!-- This makes the quest repeatable -->						
				<reward type="Item" id="vetitem_toc_daryl_name" value="3-4" />
				<reward type="Item" id="modGunBowPolymerString" value="1" />
				<reward type="Item" id="modGunMeleeTheHunter" value="1" />
				<reward type="Item" id="plantedCorn1Schematic" value="1" />
				<reward type="Item" id="ammoCrossbowBoltSteelAP" value="100-250" />
				<reward type="Item" id="ammoCrossbowBoltFlaming" value="100-250" />
				<reward type="Item" id="pouredConcreteBlock" value="10-50" />	
				<reward type="Item" id="questblock_toc_darylcrate_name" value="1" />
				<reward type="Exp" value="1000" />
				<reward type="SkillPoints" value="1" />				
			</quest>
	
	
			<!-- ELITE QUEST -->
			<quest id="+Elite Quest For Daryl's Zombiekiller Compound Crossbow">
				<property name="name_key" value="elitequest_toc_daryl_name"/>
				<property name="subtitle_key" value="elitequest_toc_daryl_subtitle"/>
				<property name="description_key" value="elitequest_toc_daryl_description"/>
				<property name="icon" value="ui_game_symbol_map_trader" />
				<property name="repeatable" value="true" />
				<property name="category_key" value="quest" />
				<property name="difficulty" value="hard" />
				<property name="offer_key" value="elitequest_toc_daryl_offer"/>
				<property name="statement_key" value="elitequest_toc_daryl_statement"/>
				<property name="response_key" value="elitequestdialog_toc_daryl"/>				
				<property name="group_name_key" value="elitequest_toc_daryl_group"/>
				<property name="shareable" value="false" />		
	
				<objective type="FetchKeep" id="gunBowT3CompoundBow" value="20" phase="1" />
				<objective type="FetchKeep" id="resourceAnimalFat" value="50" phase="1" />
				<objective type="FetchKeep" id="resourceWood" value="4000" phase="1" />
				<objective type="FetchKeep" id="resourcePaper" value="1" phase="1" />
				<objective type="Craft" id="elitewrititem_toc_daryl_name" value="1" phase="2" />		

				<reward type="Exp" value="5000" />
				<reward type="Item" id="questitem_toc_veterancard_name" value="1" />	<!-- This makes the quest repeatable -->						
			</quest>		
			
			<quest id="+Elite Reward of Daryl's Zombiekiller Compound Crossbow">
				<property name="name_key" value="elitequestreward_toc_daryl_name"/>
				<property name="subtitle_key" value="elitequestreward_toc_daryl_subtitle"/>
				<property name="description_key" value="elitequestreward_toc_daryl_description"/>
				<property name="icon" value="ui_game_symbol_map_trader" />
				<property name="repeatable" value="true" />
				<property name="category_key" value="quest" />
				<property name="difficulty" value="easy" />
				<property name="offer_key" value="elitequestreward_toc_daryl_offer"/>
				<property name="statement_key" value="elitequestreward_toc_daryl_statement"/>
				<property name="response_key" value="elitequestrewarddialog_toc_daryl"/>				
				<property name="group_name_key" value="elitequest_toc_daryl_group"/>
				<property name="shareable" value="false" />		
				<property name="completiontype" value="TurnIn" />
	
				<objective type="Goto" value="5" phase="1" />
				<objective type="ReturnToNPC">
					<property name="phase" value="2"/>
					<property name="nav_object" value="return_to_trader" />
				</objective>
				<objective type="InteractWithNPC">
					<property name="phase" value="3" />
					<property name="nav_object" value="return_to_trader" />										
				</objective>
				
				<reward type="Item" id="questitem_toc_elitecard_name" value="1" />	<!-- This makes the quest repeatable -->						
				<reward type="Item" id="eliteitem_toc_daryl_name" value="5-6" />
				<reward type="Item" id="questblock_toc_darylcrate_name" value="2" />
				<reward type="Item" id="ammoCrossbowBoltSteelAP" value="100-250" />
				<reward type="Item" id="ammoCrossbowBoltFlaming" value="100-250" />
				<reward type="Item" id="pouredConcreteBlock" value="50-100" />	
				<reward type="Exp" value="5000" />
				<reward type="SkillPoints" value="2" />				
			</quest>
			
	</append>
</config>
