<configs>
	<append xpath="/blocks">
		<!-- Create the blocks for the Sealed Citizen Crate & Citizen Crate items -->
		<block name="questblock_toc_citizencrate_name">
			<property name="DescriptionKey" value="questblock_toc_citizencrate_desc"/>			
			<property name="CustomIcon" value="cntShippingCrateHero"/>
			<property name="CustomIconTint" value="ffffb3"/>
			<property name="Extends" value="cntShippingCrateHero"/>
			<property name="DowngradeBlock" value="+Citizen Crate"/>
			<property name="FilterTags" value="floot"/>
		</block>	
		<block name="+Citizen Crate">
			<property name="CustomIcon" value="cntCardboardBox"/>
			<property name="CustomIconTint" value="333333"/>
			<property name="Extends" value="cntGarageStorage"/>
			<property name="LootList" value="365"/>
		</block>	
		<block name="questblock_toc_elitecrate_name">
			<property name="DescriptionKey" value="questblock_toc_elitecrate_desc"/>			
			<property name="CustomIcon" value="cntShippingCrateHero"/>
			<property name="CustomIconTint" value="ffffb3"/>
			<property name="Extends" value="cntShippingCrateHero"/>
			<property name="DowngradeBlock" value="+Elite Citizen Crate"/>
			<property name="FilterTags" value="floot"/>
		</block>	
		<block name="+Elite Citizen Crate">
			<property name="CustomIcon" value="cntCardboardBox"/>
			<property name="CustomIconTint" value="333333"/>
			<property name="Extends" value="cntGarageStorage"/>
			<property name="LootList" value="366"/>
		</block>	
		
		
		<block name="questblock_toc_randomcrate_name">
			<property name="DescriptionKey" value="questblock_toc_randomcrate_desc"/>			
			<property name="CustomIcon" value="cntShippingCrateHero"/>
			<property name="CustomIconTint" value="ffffb3"/>
			<property name="Extends" value="cntShippingCrateHero"/>
			<property name="DowngradeBlock" value="+Random Elite Citizen Crate"/>
			<property name="FilterTags" value="floot"/>
		</block>	
		<block name="+Random Elite Citizen Crate">
			<property name="CustomIcon" value="cntCardboardBox"/>
			<property name="CustomIconTint" value="333333"/>
			<property name="Extends" value="cntGarageStorage"/>
			<property name="LootList" value="390"/>
		</block>	
	</append>	
</configs>	
	
